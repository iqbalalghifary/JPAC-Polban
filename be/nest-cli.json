{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/accounts/src",
  "monorepo": true,
  "root": "apps/accounts",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/accounts/tsconfig.json"
  },
  "projects": {
    "accounts": {
      "type": "application",
      "root": "apps/accounts",
      "entryFile": "main",
      "sourceRoot": "apps/accounts/src",
      "compilerOptions": {
        "tsConfigPath": "apps/accounts/tsconfig.json"
      }
    },
    "emails": {
      "type": "application",
      "root": "apps/emails",
      "entryFile": "main",
      "sourceRoot": "apps/emails/src",
      "compilerOptions": {
        "tsConfigPath": "apps/emails/tsconfig.json"
      }
    },
    "vacancies": {
      "type": "application",
      "root": "apps/vacancies",
      "entryFile": "main",
      "sourceRoot": "apps/vacancies/src",
      "compilerOptions": {
        "tsConfigPath": "apps/vacancies/tsconfig.json"
      }
    },
    "portals": {
      "type": "application",
      "root": "apps/portals",
      "entryFile": "main",
      "sourceRoot": "apps/portals/src",
      "compilerOptions": {
        "tsConfigPath": "apps/portals/tsconfig.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    }
  }
}